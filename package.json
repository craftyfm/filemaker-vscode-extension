{
  "name": "filemaker-syntax",
  "displayName": "FileMaker Custom Function Syntax",
  "description": "Syntax highlighting for FileMaker Custom Functions using a TextMate grammar.",
  "version": "0.1.1",
  "publisher": "Craftyfm",
  "repository": {
  "type": "git",
  "url": "https://github.com/craftyfm/filemaker-vscode-extension.git"
},
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "filemaker",
        "aliases": [
          "FileMaker",
          "filemaker"
        ],
        "extensions": [
          ".fmfunc",
          ".filemaker",
          ".fm"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "filemaker",
        "scopeName": "source.filemaker",
        "path": "./syntaxes/filemaker.tmLanguage.json"
      }
    ]
  }
}
